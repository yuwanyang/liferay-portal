		</execute>

		<execute function="AssertTextEquals" locator1="Message#INFO" value1="Upload documents no larger than 293KB." />

		<execute macro="DMDocument#editCmd">
			<var name="dmDocumentFileEdit" value="Document_1.mp4" />
			<var name="dmDocumentTitle" value="Document_1.mp4" />
		</execute>

		<execute macro="Button#clickPublish" />

		<execute function="AssertTextEquals" locator1="Message#MAX_FILESIZE_ALERT" value1="Please enter a file with a valid file size no larger than 300000." />

		<execute macro="Button#clickCancel" />

		<execute macro="LAR#_gotoExportImport" />

		<execute function="AssertElementPresent" locator1="IFrame#DIALOG" />

		<execute macro="LAR#_export">
			<var name="exportPermissions" value="${exportPermissions}" />
			<var name="larFileName" value="DM.portlet.lar" />
		</execute>

		<execute function="SelectFrame" value1="relative=top" />
	</command>

