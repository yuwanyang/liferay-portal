	<command name="AssertFileSizeValidation" priority="4">
		<description message="This is a use case for LPS-50760." />

		<property name="test.name.skip.portal.instance" value="DocumentsadministrationUsecase#AssertFileSizeValidation" />

		<execute macro="Navigator#openURL" />

		<execute macro="ProductMenu#gotoPortlet">
