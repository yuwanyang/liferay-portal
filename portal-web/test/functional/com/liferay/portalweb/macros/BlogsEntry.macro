<definition>
	<var name="entryAuthorFullName" value="Test Test" />

	<command name="addContent">
		<execute macro="AlloyEditor#addContent">
			<var name="content" value="${entryContent}" />
		</execute>
	</command>

	<command name="addContentWithInlineImage">
		<execute macro="AlloyEditor#addContent">
			<var name="content" value="${entryContent}" />
		</execute>

		<execute macro="ItemSelector#gotoItemSelectorViaAlloyEditor" />

		<execute macro="ItemSelector#uploadFile">
			<var name="navTab" value="${navTab}" />
			<var name="uploadFileName" value="${uploadFileName}" />
		</execute>
	</command>

	<command name="addCustomAbstract">
		<execute function="Click" locator1="Radio#CUSTOM_ABSTRACT" />

		<execute macro="PortletEntry#inputDescriptionTextInput">
			<var name="description" value="${entryAbstractDescription}" />
		</execute>
	</command>

	<command name="addCustomURL">
		<execute function="Click" locator1="Radio#CUSTOM_URL" />

		<execute function="Type" locator1="TextInput#BLOGS_ENTRY_URL" value1="${customURL}" />
	</command>

	<command name="addEntryContent">
		<execute macro="BlogsEntry#addTitle">
			<var name="entryTitle" value="${entryTitle}" />
		</execute>

		<if>
			<isset var="entrySubtitle" />
			<then>
				<execute macro="BlogsEntry#addSubtitle">
					<var name="entrySubtitle" value="${entrySubtitle}" />
				</execute>
			</then>
		</if>

		<execute macro="BlogsEntry#addContent">
			<var name="entryContent" value="${entryContent}" />
		</execute>
	</command>

	<command name="addEntryExternalLink">
		<execute function="SelectFieldText" locator1="AlloyEditor#EDITOR">
			<var name="key_editor" value="content" />
		</execute>

		<execute function="Click" locator1="AlloyEditor#TEXT_FORMATTING_LINK" />

		<var name="externalLink" value="${externalLink}" />

		<execute function="Type" locator1="AlloyEditor#TEXT_FORMATTING_LINK_INPUT" value1="${externalLink}" />

		<execute function="Click" locator1="AlloyEditor#TEXT_FORMATTING_LINK_INPUT_CONFIRM" />
	</command>

	<command name="addRelatedAsset">
		<execute function="AssertClick" locator1="AssetRelatedAssets#RELATED_ASSETS_SELECT" value1="Select" />

		<var name="key_assetType" value="${assetType}" />

		<execute function="AssertClick" locator1="AssetRelatedAssets#RELATED_ASSETS_SELECT_MENULIST_ASSET" value1="${assetType}" />

		<var name="key_assetTitle" value="${assetTitle}" />

		<execute function="SelectFrame" locator1="AssetRelatedAssets#SELECT_ASSET_IFRAME" />

		<execute function="AssertClick" locator1="AssetRelatedAssets#ASSET_TABLE_CHOOSE" value1="${assetTitle}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addSmallImage">
		<execute function="Click" locator1="Button#SELECT_FILE_SMALL_IMAGE" />

		<execute macro="ItemSelector#uploadFile">
			<var name="navTab" value="${navTab}" />
			<var name="uploadFileName" value="${uploadFileName}" />
		</execute>
	</command>

	<command name="addSubtitle">
		<execute macro="AlloyEditor#typeEditor">
			<var name="content" value="${entrySubtitle}" />
			<var name="editor" value="subtitle" />
		</execute>
	</command>

	<command name="addTag">
		<var name="key_tagName" value="${tagName}" />

		<execute function="Type" locator1="AssetCategorization#TAGS_FIELD" value1="${tagName}" />

		<execute function="AssertClick" locator1="Button#ADD_TAGS" value1="Add" />

		<execute function="AssertTextEquals" locator1="AssetCategorization#TAG" value1="${tagName}" />
	</command>

	<command name="addTitle">
		<execute macro="AlloyEditor#addTitle">
			<var name="title" value="${entryTitle}" />
		</execute>
	</command>

	<command name="addWithTrackbackURLPG">
		<execute macro="BlogsNavigator#gotoEntryPG">
			<var name="entryContent" value="${entryContent}" />
			<var name="entryTitle" value="${entryTitle}" />
		</execute>

		<var name="key_entryContent" value="${entryContent}" />
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute macro="BlogsEntry#viewPG">
			<var name="entryContent" value="${entryContent}" />
			<var name="entryTitle" value="${entryTitle}" />
		</execute>

		<execute function="Pause" locator1="3000" />

		<execute function="Copy" locator1="BlogsEntry#COMMENTS_TRACKBACK_URL" />

		<if>
			<equals arg1="${virtualHosts}" arg2="true" />
			<then>
				<execute macro="Navigator#gotoSpecificURLPage">
					<var name="pageName" value="Blogs Page" />
					<var name="url" value="${virtualHostsURL}" />
				</execute>
			</then>
			<else>
				<execute macro="Navigator#gotoPage">
					<var name="pageName" value="Blogs Page" />
				</execute>
			</else>
		</if>

		<execute macro="BlogsNavigator#gotoAddEntry" />

		<execute macro="BlogsEntry#addEntryContent">
			<var name="entryContent" value="${entryContent2}" />
			<var name="entryTitle" value="${entryTitle2}" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Configuration" />
		</execute>

		<execute function="Paste" locator1="TextInput#TRACKBACKS" />

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="addWithWorkflowPG">
		<execute macro="BlogsNavigator#gotoAddEntry" />

		<execute macro="BlogsEntry#addEntryContent">
			<var name="entryContent" value="${entryContent}" />
			<var name="entryTitle" value="${entryTitle}" />
		</execute>

		<execute function="AssertClick" locator1="Button#SUBMIT_FOR_PUBLICATION" value1="Submit for Publication" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="autoSaveDraft">
		<execute function="Pause" locator1="30000" />

		<execute function="AssertElementPresent" locator1="Message#DRAFT_AUTO_SAVE" />
	</command>

	<command name="editContent">
		<execute function="AssertTextEquals" locator1="AlloyEditor#EDITOR" value1="${entryContent}">
			<var name="key_editor" value="content" />
		</execute>

		<execute macro="AlloyEditor#addContent">
			<var name="content" value="${entryContentEdit}" />
		</execute>
	</command>

	<command name="editContentViaCardViewPG">
		<execute function="MouseOver" locator1="Portlet#PORTLET_CONTENT" />

		<execute macro="LexiconEntry#gotoEllipsisMenuItem">
			<var name="menuItem" value="Edit" />
		</execute>

		<execute macro="BlogsEntry#editContent">
			<var name="entryContent" value="${entryContent}" />
			<var name="entryContentEdit" value="${entryContentEdit}" />
		</execute>
	</command>

	<command name="editDisplayDate">
		<var method="DateUtil#getCurrentDate()" name="displayDateDay" />
		<var method="DateUtil#getCurrentMonth()" name="displayDateMonth" />
		<var method="DateUtil#getCurrentYear()" name="displayDateYear" />

		<var name="displayDate" value="${displayDateMonth}/${displayDateDay}/${displayDateYear}" />

		<execute function="Type" locator1="TextInput#DISPLAY_DATE" value1="${displayDate}" />

		<var method="DateUtil#getCurrentHour()" name="displayTimeHour" />

		<var name="displayTimeMinute" value="${displayTimeMinute}" />

		<var name="displayTime" value="${displayTimeHour}:${displayTimeMinute}" />

		<execute function="Type" locator1="TextInput#DISPLAY_DATE_TIME" value1="${displayTime}" />
	</command>

	<command name="editDisplayDateYear">
		<var method="selenium#getElementValue('TextInput#DISPLAY_DATE_DAY')" name="displayDateDay" />
		<var method="selenium#getElementValue('TextInput#DISPLAY_DATE_MONTH')" name="displayDateMonth" />
		<var method="DateUtil#getCurrentYear()" name="displayDateYear" />

		<var method="MathUtil#sum('${displayDateYear}', '1')" name="displayDateYear" />

		<var name="displayDate" value="${displayDateMonth}/${displayDateDay}/${displayDateYear}" />

		<execute function="Type" locator1="TextInput#DISPLAY_DATE" value1="${displayDate}" />
	</command>

	<command name="editEntryContent">
		<execute macro="BlogsEntry#editTitle">
			<var name="entryTitle" value="${entryTitle}" />
			<var name="entryTitleEdit" value="${entryTitleEdit}" />
		</execute>

		<if>
			<isset var="entrySubtitleEdit" />
			<then>
				<execute macro="BlogsEntry#editSubtitle">
					<var name="entrySubtitle" value="${entrySubtitle}" />
					<var name="entrySubtitleEdit" value="${entrySubtitleEdit}" />
				</execute>
			</then>
		</if>

		<execute macro="BlogsEntry#editContent">
			<var name="entryContent" value="${entryContent}" />
			<var name="entryContentEdit" value="${entryContentEdit}" />
		</execute>
	</command>

	<command name="editNoPermissionPG">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="BlogsEntry#TITLE" value1="${entryTitle}" />

		<var name="key_entryContent" value="${entryContent}" />

		<execute function="AssertTextEquals" locator1="BlogsEntry#CONTENT" value1="${entryContent}" />

		<execute macro="Portlet#mouseOverPortletTitle">
			<var name="portletTitleName" value="Blogs" />
		</execute>

		<execute function="Click" locator1="Icon#BODY_VERTICAL_ELLIPSIS" />

		<execute macro="MenuItem#viewNotPresent">
			<var name="menuItem" value="Edit" />
		</execute>
	</command>

	<command name="editSubtitle">
		<execute function="AssertTextEquals" locator1="AlloyEditor#EDITOR" value1="${entrySubtitle}">
			<var name="key_editor" value="subtitle" />
		</execute>

		<execute macro="AlloyEditor#typeEditor">
			<var name="content" value="${entrySubtitleEdit}" />
			<var name="editor" value="subtitle" />
		</execute>
	</command>

	<command name="editTitle">
		<execute function="AssertTextEquals" locator1="AlloyEditor#EDITOR" value1="${entryTitle}">
			<var name="key_editor" value="title" />
		</execute>

		<execute macro="AlloyEditor#addTitle">
			<var name="title" value="${entryTitleEdit}" />
		</execute>
	</command>

	<command name="entryAddPermissions">
		<if>
			<isset var="viewableBy" />
			<then>
				<execute function="Select" locator1="AssetPermissions#VIEWABLE_AS_SELECT" value1="${viewableBy}" />
			</then>
		</if>

		<if>
			<isset var="permissionDefinitionKey" />
			<then>
				<if>
					<condition function="IsNotVisible" locator1="AssetPermissions#HIDE_OPTIONS_LINK" />
					<then>
						<execute function="AssertClick#assertPartialTextClickAt" locator1="AssetPermissions#MORE_OPTIONS_LINK" value1="More Options" />
					</then>
				</if>

				<var name="key_permissionDefinitionKey" value="${permissionDefinitionKey}" />
				<var name="key_roleTitle" value="${roleTitle}" />

				<execute function="Check" locator1="AssetPermissions#PERMISSIONS_CHECKBOX" />
			</then>
		</if>
	</command>

	<command name="flagPG">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="Click" locator1="BlogsEntry#FLAG_LINK" />

		<execute function="AssertElementPresent#assertVisible" locator1="BlogsEntry#FLAG_MODAL" />
		<execute function="AssertTextEquals" locator1="ReportContent#WARNING" value1="You are about to report a violation of our Terms of Use. All reports are strictly confidential." />
		<execute function="AssertTextEquals" locator1="ReportContent#WARNING_TERMS_OF_USE_LINK" value1="Terms of Use" />

		<execute function="Select" locator1="ReportContent#REASON_SELECT" value1="${flagReason}" />

		<execute function="AssertClick" locator1="Button#REPORT" value1="Report" />

		<execute function="AssertTextEquals#assertPartialText" locator1="ReportContent#SUCCESS_MESSAGE" value1="Although we cannot disclose our final decision, we do review every report and appreciate your effort to make sure ${siteName} is a safe environment for everyone." />

		<execute function="Click" locator1="Button#CLOSE" />
	</command>

	<command name="moveToRecycleBinCP">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="Blogs#DESCRIPTIVE_VIEW_ENTRY_TITLE" value1="${entryTitle}" />

		<execute function="Click" locator1="Blogs#DESCRIPTIVE_VIEW_ENTRY_OPTIONS_DROPDOWN" />

		<execute macro="PortletEntry#clickMoveToRecycleBin" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="moveToRecycleBinDraftCP">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="Blogs#DESCRIPTIVE_VIEW_ENTRY_TITLE" value1="${entryTitle}" />
		<execute function="AssertTextEquals" locator1="Blogs#DESCRIPTIVE_VIEW_ENTRY_STATUS" value1="Draft" />

		<execute function="Click" locator1="Blogs#DESCRIPTIVE_VIEW_ENTRY_OPTIONS_DROPDOWN" />

		<execute macro="PortletEntry#clickMoveToRecycleBin" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="moveToRecycleBinNoPermissionPG">
		<execute function="AssertElementNotPresent" locator1="Link#MOVE_TO_RECYCLING_BIN" />
	</command>

	<command name="moveToRecycleBinPG">
		<var name="key_entryContent" value="${entryContent}" />
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="BlogsEntry#TITLE" value1="${entryTitle}" />
		<execute function="AssertTextEquals" locator1="BlogsEntry#CONTENT" value1="${entryContent}" />

		<execute macro="Portlet#mouseOverPortletTitle">
			<var name="portletTitleName" value="Blogs" />
		</execute>

		<execute macro="LexiconEntry#gotoEllipsisMenuItem">
			<var name="menuItem" value="Move to Recycle Bin" />
		</execute>

		<execute function="AssertElementPresent" locator1="Message#SUCCESS" />

		<execute function="AssertElementNotPresent" locator1="BlogsEntry#TITLE" />
		<execute function="AssertElementNotPresent" locator1="BlogsEntry#CONTENT" />
	</command>

	<command name="publishWithInvalidContent">
		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute macro="Alert#viewRequiredField" />
		<execute function="AssertElementNotPresent" locator1="Message#SUCCESS" />
	</command>

	<command name="publishWithInvalidTitle">
		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="AssertTextEquals#assertPartialText" locator1="BlogsEntry#TITLE_FIELD_HELPER" value1="This field is required to publish the entry." />
		<execute function="AssertElementNotPresent" locator1="Message#SUCCESS" />
	</command>

	<command name="publishWithInvalidTitleLength">
		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute macro="Alert#viewRequestFailedToComplete" />

		<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_ENTER_A_VALID_TITLE" value1="Please enter a valid title." />
	</command>

	<command name="publishWithWorkflow">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertClick" locator1="Button#SUBMIT_FOR_PUBLICATION" value1="Submit for Publication" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="rateThumbsDown">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="BlogsEntry#TITLE" value1="${entryTitle}" />

		<execute function="Click" locator1="Ratings#THUMB_DOWN" />
	</command>

	<command name="rateThumbsUp">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="BlogsEntry#TITLE" value1="${entryTitle}" />

		<execute function="Click" locator1="Ratings#THUMB_UP" />
	</command>

	<command name="removeSmallImage">
		<execute function="Click" locator1="Button#REMOVE_IMAGE" />

		<execute function="AssertElementNotPresent" locator1="BlogsEntry#SMALL_IMAGE">
			<var name="key_smallImage" value="${uploadFileName}" />
		</execute>

		<execute function="AssertTextEquals" locator1="Button#SELECT" value1="Select File" />
	</command>

	<command name="removeViewPermissionsPG">
		<var method="StringUtil#replace('${roleTitle}', ' ', '-')" name="key_roleTitle" />
		<var method="StringUtil#lowerCase('${key_roleTitle}')" name="key_roleTitle" />

		<execute macro="Portlet#mouseOverPortletTitle">
			<var name="portletTitleName" value="Blogs" />
		</execute>

		<execute function="Click" locator1="Icon#BODY_VERTICAL_ELLIPSIS" />

		<execute macro="MenuItem#click">
			<var name="menuItem" value="Permissions" />
		</execute>

		<execute macro="IFrame#selectPermissionsFrame" />

		<execute function="Uncheck" locator1="Permissions#CONTENT_PERMISSIONS_VIEW_CHECKBOX" value1="${roleTitle}" />

		<execute macro="Button#clickSave" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="saveAsDraft">
		<execute function="AssertClick" locator1="Button#SAVE_AS_DRAFT" value1="Save as Draft" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="scheduleBlogEntry">
		<var method="selenium#getElementValue('TextInput#DISPLAY_DATE_DAY')" name="displayDateDay" />
		<var method="selenium#getElementValue('TextInput#DISPLAY_DATE_MONTH')" name="displayDateMonth" />

		<var method="MathUtil#sum('${displayDateMonth}', '1')" name="displayDateMonth" />

		<var method="selenium#getElementValue('TextInput#DISPLAY_DATE_YEAR')" name="displayDateYear" />

		<var name="displayDate" value="${displayDateMonth}/${displayDateDay}/${displayDateYear}" />

		<execute function="Type" locator1="TextInput#DISPLAY_DATE" value1="${displayDate}" />

		<var method="selenium#getElementValue('TextInput#DISPLAY_DATE_HOUR')" name="displayDateHour" />

		<var method="StringUtil#length('${displayDateHour}')" name="displayDateHourLength" />

		<if>
			<equals arg1="${displayDateHourLength}" arg2="1" />
			<then>
				<var name="displayDateHour" value="0${displayDateHour}" />
			</then>
		</if>

		<var method="selenium#getElementValue('TextInput#DISPLAY_DATE_MINUTE')" name="displayDateMinute" />

		<var method="MathUtil#sum('${displayDateMinute}', '3')" name="displayDateMinuteFuture" />

		<var method="StringUtil#length('${displayDateMinuteFuture}')" name="displayDateMinuteFutureLength" />

		<if>
			<equals arg1="${displayDateMinuteFutureLength}" arg2="1" />
			<then>
				<var name="displayDateMinuteFuture" value="0${displayDateMinuteFuture}" />
			</then>
		</if>

		<var method="selenium#getElementValue('TextInput#DISPLAY_DATE_MERIDIAN')" name="displayDateMeridian" />

		<var method="StringUtil#replace('${displayDateMeridian}', '0', 'AM')" name="displayDateMeridian" />
		<var method="StringUtil#replace('${displayDateMeridian}', '1', 'PM')" name="displayDateMeridian" />

		<var name="displayTime" value="${displayDateHour}:${displayDateMinuteFuture} ${displayDateMeridian}" />

		<execute function="Type" locator1="TextInput#DISPLAY_DATE_TIME" value1="${displayTime}" />
	</command>

	<command name="searchCP">
		<var name="key_entryContent" value="${entryContent}" />
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute macro="Search#searchCP">
			<var name="searchTerm" value="${entrySearchTerm}" />
		</execute>

		<execute function="AssertTextEquals" locator1="Blogs#DESCRIPTIVE_VIEW_ENTRY_TITLE" value1="${entryTitle}" />
	</command>

	<command name="searchNoCP">
		<var name="key_entryContent" value="${entryContent}" />
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute macro="Search#searchCP">
			<var name="searchTerm" value="${entrySearchTerm}" />
		</execute>

		<execute function="AssertTextEquals" locator1="Message#EMPTY_INFO" value1="No entries were found." />
		<execute function="AssertElementNotPresent" locator1="Blogs#DESCRIPTIVE_VIEW_ENTRY_TITLE" />
	</command>

	<command name="selectFormattingStyle">
		<execute function="SelectFieldText" locator1="AlloyEditor#EDITOR">
			<var name="key_editor" value="content" />
		</execute>

		<execute function="Click" locator1="AlloyEditor#TEXT_FORMATTING_STYLE" />

		<execute function="Click" locator1="AlloyEditor#TEXT_FORMATTING_STYLE_DROPDOWN">
			<var name="key_styleFormat" value="${styleFormat}" />
		</execute>
	</command>

	<command name="tearDownCP">
		<execute macro="Navigator#openURL" />

		<execute macro="ProductMenu#gotoPortlet">
			<var name="category" value="Content" />
			<var name="panel" value="Site Administration" />
			<var name="portlet" value="Blogs" />
		</execute>

		<if>
			<condition function="IsElementPresent" locator1="Message#EMPTY_INFO" value1="No entries were found." />
			<then>
				<execute macro="Blogs#viewNoEntryCP" />
			</then>
			<else>
				<execute macro="PortletEntry#selectAll" />

				<execute function="Click" locator1="Icon#MOVE_TO_RECYCLE_BIN" />

				<execute function="AssertElementPresent" locator1="Message#SUCCESS" />
			</else>
		</if>

		<execute macro="Navigator#openURL" />

		<execute macro="ProductMenu#gotoPortlet">
			<var name="category" value="Recycle Bin" />
			<var name="panel" value="Site Administration" />
			<var name="portlet" value="Recycle Bin" />
		</execute>

		<execute macro="RecycleBin#emptyCP" />
	</command>

	<command name="viewAbstractDisplay">
		<var name="key_entryAbstractDescription" value="${entryAbstractDescription}" />
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="BlogsEntry#TITLE" value1="${entryTitle}" />
		<execute function="AssertTextEquals#assertPartialText" locator1="BlogsEntry#CONTENT_ABSTRACT" value1="${entryAbstractDescription}" />
		<execute function="AssertTextNotEquals" locator1="BlogsEntry#CONTENT_GENERIC" value1="${entryContent}" />
	</command>

	<command name="viewAsRelatedAssetPG">
		<var name="key_assetTitle" value="${assetTitle}" />
		<var name="key_entryContent" value="${entryContent}" />
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="BlogsEntry#TITLE" value1="${entryTitle}" />
		<execute function="AssertTextEquals" locator1="BlogsEntry#CONTENT" value1="${entryContent}" />
		<execute function="AssertTextEquals" locator1="AssetRelatedAssets#HEADER" value1="Related Assets:" />
		<execute function="AssertTextEquals" locator1="AssetRelatedAssets#ASSET_TITLE" value1="${assetTitle}" />
	</command>

	<command name="viewContent">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="Blogs#ICON_VIEW_ENTRY_TITLE" value1="${entryTitle}" />
	</command>

	<command name="viewDefaultCP">
		<execute function="AssertTextEquals" locator1="Message#EMPTY_INFO" value1="No entries were found." />
		<execute function="AssertElementNotPresent" locator1="Blogs#ENTRY_TABLE_TITLE" />
	</command>

	<command name="viewEntry">
		<var name="externalURL" value="${externalURL}" />
		<var name="key_entryContent" value="${entryContent}" />
		<var name="key_entryExternalLink" value="${entryExternalLink}" />
		<var name="key_entrySubtitle" value="${entrySubtitle}" />
		<var name="key_entryTitle" value="${entryTitle}" />
		<var name="key_uploadFileName" value="${uploadFileName}" />

		<execute function="Pause" locator1="3000" />

		<execute function="AssertTextEquals" locator1="BlogsEntry#TITLE" value1="${entryTitle}" />

		<if>
			<isset var="entrySubtitle" />
			<then>
				<execute function="AssertTextEquals" locator1="BlogsEntry#SUBTITLE" value1="${entrySubtitle}" />
			</then>
		</if>

		<if>
			<isset var="uploadFileName" />
			<then>
				<execute function="AssertElementPresent" locator1="BlogsEntry#ENTRY_COVER_IMAGE" />
			</then>
		</if>

		<if>
			<isset var="entryContent" />
			<then>
				<execute function="AssertTextEquals" locator1="BlogsEntry#CONTENT" value1="${entryContent}" />
			</then>
		</if>

		<if>
			<isset var="entryContentBold" />
			<then>
				<execute function="AssertTextEquals#assertPartialText" locator1="BlogsEntry#CONTENT_HTML_BOLD" value1="${entryContentBold}" />
			</then>
		</if>

		<if>
